<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Support for implementing the repository pattern when storing data using Azure Storage Tables or Azure Cosmos DB</Description>
    <Copyright>Byron W Jones LLC</Copyright>
    <DebugType>embedded</DebugType>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <Version>2.1.3</Version>
    <Authors>Byron W Jones</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Azure.Data.Tables" Version="12.9.0" />
    <PackageReference Include="BWJ.Core" Version="1.2.0" />
  </ItemGroup>

</Project>
